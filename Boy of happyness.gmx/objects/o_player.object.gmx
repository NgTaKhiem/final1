    image_xscale = sign(move);
}

x += moveX;
y += moveY;

if (place_meeting(x, y, o_reaper_bullet)) {
    if (!isDamage) {
        global.hp -= 5;
        isDamage = true;
        alarm[0] = 60;
    }
    
